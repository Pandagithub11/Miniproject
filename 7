App.js

import './App.css';
import Hello from './mycomponent/Hello';
import Bye from './mycomponent/Bye';

function App() {
  return (
    <div className="App">
      <Hello/>
      <Bye/>
    </div>
  );
}


export default App;

Hello.js

import React from 'react'

